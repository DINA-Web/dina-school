{
  "env": {
    "jest/globals": true
  },
  "extends": [
    "eslint-config-airbnb-base",
    "prettier"
  ],
  "plugins": [
    "jest",
    "prettier"
  ],
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "indent": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-console": 2,
    "prefer-destructuring": 1,
    "prettier/prettier": "error"
  }
}
